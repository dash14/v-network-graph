{
  "name": "v-network-graph",
  "description": "An interactive network graph visualization component for Vue 3",
  "version": "0.6.10",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "license": "MIT",
  "homepage": "https://dash14.github.io/v-network-graph/",
  "repository": {
    "type": "git",
    "url": "https://github.com/dash14/v-network-graph.git"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "run-s clean build:tc build:lib",
    "build:tc": "vue-tsc --noEmit",
    "build:lib": "run-p build:lib:*",
    "build:lib:main": "vite build",
    "build:lib:force": "vite --config vite-lib-force.config.ts build",
    "clean": "rimraf lib",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@dash14/svg-pan-zoom": "^3.6.9",
    "mitt": "^3.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.19.0",
    "@storybook/addon-essentials": "^7.0.0-alpha.41",
    "@storybook/addon-interactions": "^7.0.0-alpha.41",
    "@storybook/addon-links": "^7.0.0-alpha.41",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^7.0.0-alpha.41",
    "@storybook/vue3-vite": "^7.0.0-alpha.41",
    "@types/d3-force": "^3.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.21",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/compiler-sfc": "^3.2.39",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "storybook": "^7.0.0-alpha.41",
    "typescript": "^4.7.4",
    "vite": "^3.1.3",
    "vite-plugin-dts": "^1.5.0",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.13"
  },
  "peerDependencies": {
    "d3-force": "^3.0.0",
    "vue": "^3.2.31"
  },
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./lib/force-layout": {
      "import": "./lib/force-layout.mjs",
      "require": "./lib/force-layout.js"
    },
    "./lib/*": "./lib/*"
  }
}
